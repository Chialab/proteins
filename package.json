{
  "name": "@chialab/proteins",
  "version": "3.2.5",
  "description": "A primer for JavaScript libraries and frameworks development.",
  "lib": "src/index.js",
  "main": "dist/cjs/proteins.js",
  "module": "dist/esm/proteins.js",
  "browser": "dist/umd/proteins.js",
  "directories": {
    "src": "src",
    "test": "test",
    "dist": "dist"
  },
  "scripts": {
    "build": "rm -rf dist && rna build --production --typings",
    "test": "yarn test:browser && yarn test:node",
    "test:browser": "rna unit 'test/*.js' --coverage --browser",
    "test:saucelabs": "rna unit 'test/*.js' --coverage --saucelabs --concurrency 2",
    "test:node": "rna unit 'test/*.js' --coverage --node",
    "lint": "rna lint 'src/**/*.js'",
    "docs": "rna documentate --output docs/API.md --header '# API Reference'",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:chialab/proteins.git"
  },
  "keywords": [
    "proteins",
    "chialab",
    "javascript",
    "utils"
  ],
  "author": "Chialab <dev@chialab.io> (https://www.chialab.it)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chialab/proteins/issues"
  },
  "homepage": "https://chialab.io/p/proteins",
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "chai": "^4.1.2",
    "eslint": "^6.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-mocha": "^6.2.1"
  }
}
